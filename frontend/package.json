    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^25.0.10",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^4.7.0",
    "jsdom": "^24.1.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "terser": "^5.44.0",
    "typescript": "^5.9.3",
