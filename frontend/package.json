{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:dev": "vite build --minify false --mode development",
    "build": "tsc && vite build --mode production",
    "preview": "vite preview",
    "test": "vitest",
    "test:hotkeys": "vitest run src/__tests__ --test-timeout=5000",
    "test:hotkeys:watch": "vitest src/__tests__ --test-timeout=5000",
    "test:coverage": "vitest run --coverage src/__tests__",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "format:check": "biome format ."
  },
  "dependencies": {
    "@blocknote/code-block": "^0.43.0",
    "@blocknote/core": "^0.43.0",
    "@blocknote/react": "^0.43.0",
    "@blocknote/shadcn": "^0.43.0",
    "@mantine/hooks": "^8.3.9",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-switch": "^1.1.3",
    "@tailwindcss/forms": "^0.5.10",
    "@wailsio/runtime": "^3.0.0-alpha.73",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "fuse.js": "^7.1.0",
    "lucide-react": "^0.563.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.3.1",
